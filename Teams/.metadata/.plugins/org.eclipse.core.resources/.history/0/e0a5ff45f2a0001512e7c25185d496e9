package squads.engineer;

import edu.warbot.agents.agents.WarEngineer;
import edu.warbot.agents.enums.WarAgentType;
import squads.WarEngineerBrainController;
import squads.fsm.Fsm;
import squads.fsm.MovableAgentState;

public class EngineerState extends MovableAgentState
{
	WarEngineerBrainController webc;
	
	public EngineerState(Fsm fsm, WarEngineerBrainController webc)
	{
		super(fsm, webc);
		this.webc = webc;
	}
	
	public String execute()
	{
		webc.setDebugString("What am I doing ??");
		webc.setNextBuildingToBuild(WarAgentType.Wall);
        return WarEngineer.ACTION_BUILD;
	}
	
	public void reflexe()
	{
		
	}
	
	public void update()
	{
		
	}
}
